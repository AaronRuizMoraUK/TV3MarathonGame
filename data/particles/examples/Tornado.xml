<SYSTEM_DEFINITION name = "Tornado">
	<SUBSYSTEM max = "100" particleColorFactor = "1.0" >
		<TEXTURE diffuse = "particula2.dds" />
		<SOURCE particle_rate = "20">
			<DOMAIN type = "DISC" 
				x0 = "0.0"
				y0 = "2.0"
				z0 = "0.0"
				normalx = "0.0"
			        normaly = "1.0" 
				normalz = "0.0"

				OuterRadius = "6.0"
				InnerRadius = "5.9"
				rangAngle = "3.14"
				phase = "1.57" />
			<COLOR>
				<DOMAIN type = "POINT" 
					x0 = "0.5"
					z0 = "1.0"
					y0 = "0.5" />
				<DOMAIN type = "POINT" 
					x0 = "0.5"
					z0 = "0.5"
					y0 = "0.5" />
			</COLOR>

			<VELOCITY>
				<DOMAIN type = "POINT" 
					x0 = "0.0"
					z0 = "0.0"
					y0 = "0.0" />
			</VELOCITY>

			<SIZE>
				<DOMAIN type = "POINT" 
					x0 = "1.5"
					z0 = "1.5"
					y0 = "1.5" />
			</SIZE>
		</SOURCE>

		<GRAVITY>
			<DOMAIN type = "POINT" 
				x0 = "0.0"
				z0 = "0.0"
				y0 = "5.0" />
		</GRAVITY>


		<AIRTUBE vRot = "5.0" vIn = "2.0">
			<DOMAIN type = "POINT" 
				x0 = "0.0"
				z0 = "0.0"
				y0 = "0.0" />
		</AIRTUBE>

		<KILLOLDALPHA age_limit = "2.0" scale = "1.0" />

		<MOVE move_velocity = "true" move_rotational_velocity = "true" />
	</SUBSYSTEM>
	<SUBSYSTEM max = "40" particleColorFactor = "1.0" >
